{
  "manifest_version": 2,
  "name": "Encode Utility.",
  "version": "0.0.2",
  "description": "Encode Utility.",
  "author": "yama-dev",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "js": ["content_script.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ]
}
